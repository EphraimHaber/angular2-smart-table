{
  "name": "ngx-smart-table",
  "version": "1.7.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:demo": "ng build demo --aot --configuration production",
    "build:lib": "ng build ng2-smart-table --configuration production",
    "build:ci": "npm run build:demo && npm run build:lib",
    "docs:gh-pages": "npm run build:demo -- --base-href '/ng2-smart-table/' && ngh --dir ./dist/demo",
    "test": "ng test",
    "release": "npm run build:lib && npm run publish",
    "publish": "npm publish --access=public dist/ng2-smart-table",
    "changelog": "standard-changelog -p angular",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:stats": "ng build --configuration production --aot --stats-json",
    "analyze": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.7",
    "@angular/common": "~12.2.7",
    "@angular/compiler": "~12.2.7",
    "@angular/core": "~12.2.7",
    "@angular/forms": "~12.2.7",
    "@angular/platform-browser": "~12.2.7",
    "@angular/platform-browser-dynamic": "~12.2.7",
    "@angular/router": "~12.2.7",
    "highlight.js": "^10.4.1",
    "lodash-es": "4.17.21",
    "ng2-completer": "^9.0.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "raw-loader": "^0.5.1",
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular/cli": "~12.2.7",
    "@angular/compiler-cli": "~12.2.7",
    "@angular/language-service": "~12.2.7",
    "@types/node": "^12.18.4",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.1",
    "jasmine-spec-reporter": "~6.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "ng-packagr": "^11.0.3",
    "protractor": "~7.0.0",
    "standard-changelog": "^2.0.27",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "@types/jasmine": "~3.8.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}
